<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import View.WinCharDisplay?>
<?import View.WinGolemDisplay?>
<AnchorPane fx:id="WinPane" stylesheets="@MainMenuStyle.css" xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="View.WinController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <WinGolemDisplay fx:id="winGolemDisplayer" height="${WinPane.height}" width="${WinPane.width}" > </WinGolemDisplay>
        <WinCharDisplay fx:id="winCharDisplayer" height="${WinPane.height}" width="${WinPane.width}" > </WinCharDisplay>
        <Label id="lbl_win" fx:id="lbl_win" contentDisplay="CENTER" layoutX="180.0" layoutY="50.0" prefHeight="150.0" prefWidth="494.0" text="YOU WIN" textFill="WHITE">
            <font>
                <Font name="Arial Bold" size="100.0" />
            </font>
        </Label>
    </children>
</AnchorPane>
